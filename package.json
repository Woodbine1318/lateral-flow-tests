{
  "name": "micro-mobility-news",
  "private": true,
  "description": "Micro Mobility News Website",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ignore-path .gitignore",
    "prettier": "prettier . --ignore-path .gitignore",
    "check-format": "yarn prettier --list-different",
    "format": "yarn prettier --write",
    "validate": "yarn check-format && yarn lint && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/barlow": "^4.5.0",
    "@fontsource/barlow-condensed": "^4.5.0",
    "@fontsource/roboto": "^4.5.1",
    "@fontsource/roboto-condensed": "^4.5.0",
    "@reach/dialog": "^0.16.2",
    "autoprefixer": "^10.3.7",
    "gatsby": "^3.14.2",
    "gatsby-plugin-disqus": "^1.2.3",
    "gatsby-plugin-feed": "^4.0.0",
    "gatsby-plugin-google-gtag": "^4.3.0",
    "gatsby-plugin-image": "^1.14.1",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-postcss": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sharp": "^3.14.1",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-source-contentful": "^5.14.1",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "markdown-to-jsx": "^7.1.5",
    "postcss": "^8.3.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.2.17"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "gatsby-plugin-eslint": "^4.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.4.1"
  },
  "license": "0BSD",
  "lint-staged": {
    "*.+(js|json|jsx)": [
      "prettier --write",
      "prettier --list-different"
    ],
    "*.(js|jsx)": [
      "eslint"
    ]
  }
}
